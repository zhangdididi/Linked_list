class Solution {
    public ListNode middleNode(ListNode head) {
        int len = 0;
        for (ListNode cur = head ; cur != null; cur = cur.next) {
            //求链表长度
            len++;
        }
        for (int i = 0; i < len / 2; i++) {
            //让节点走一半长度
            head = head.next;
        }
        return head;
    }
}
